{
  "name": "image-sprite-webpack-plugin",
  "version": "1.0.0-alpha",
  "description": "Image Sprite Plugin for Webpack.",
  "main": "dist/cjs/ImageSpritePlugin.js",
  "module": "dist/esm/ImageSpritePlugin.js",
  "types": "dist/ImageSpritePlugin.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "yarn lint && yarn coverage && yarn compile && yarn test",
    "clean": "rimraf dist",
    "compile": "yarn clean && yarn compile:cjs && yarn compile:esm && yarn compile:types",
    "compile:cjs": "tsc -p config/tsconfig.cjs.json",
    "compile:esm": "tsc -p config/tsconfig.esm.json",
    "compile:types": "tsc --declaration --emitDeclarationOnly --declarationMap --outDir dist",
    "coverage": "jest --coverage",
    "coveralls": "cat .coverage/lcov.info | coveralls",
    "dev": "jest --watchAll",
    "lint": "eslint . --ext .ts",
    "performance": "node -r ts-node/register ./test/usages/performance.ts",
    "prepare": "husky install",
    "profile": "node -r ts-node/register --inspect ./test/usages/profile.ts",
    "publish": "yarn run build && yarn config set registry https://registry.yarnjs.com/ && yarn publish",
    "publish-test": "yarn run build && yarn config set registry http://localhost:4873/ && yarn publish --registry http://localhost:4873",
    "test": "jest --ci",
    "help": "tsc --help"
  },
  "author": "Shim Heungwoon <ajaxlab7@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/css": "^0.0.33",
    "@types/vinyl": "^2.0.7",
    "chalk": "^5.2.0",
    "css": "^3.0.0",
    "figures": "^5.0.0",
    "spritesmith": "^3.4.0",
    "vinyl": "^3.0.0",
    "webpack-sources": "^3.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-node": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "webpack": "^5.75.0"
  },
  "lint-staged": {
    "*.{html,js,ts}": "eslint --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/image-sprite-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "webpack-plugin",
    "stylesheet",
    "css-loader",
    "css",
    "spritesmith"
  ]
}
